{
    "name": "website",
    "type": "module",
    "version": "2.0.0",
    "scripts": {
        "dev": "astro dev",
        "start": "astro preview",
        "build": "astro build",
        "typecheck": "tsc --noEmit",
        "typecheck:w": "npm run typecheck -- --watch",
        "check": "astro check",
        "check:w": "npm run check -- --watch",
        "format": "prettier -w src config",
        "format:check": "prettier --check src/**/*.{js,ts,tsx,astro} config/**/*.ts",
        "lint": "eslint --ext .js,.ts,.tsx,.astro src config",
        "ci:setup-vercel": "vercel pull --yes --environment production --token \"$VERCEL_TOKEN\"",
        "ci:deploy": "vercel deploy --prebuilt --prod --token \"$VERCEL_TOKEN\""
    },
    "dependencies": {
        "@astrojs/mdx": "2.3.1",
        "@astrojs/tailwind": "5.1.0",
        "@astrojs/vercel": "7.7.2",
        "@fontsource-variable/inter": "5.0.18",
        "astro": "4.11.3",
        "astro-icon": "1.1.0",
        "clsx": "2.1.1",
        "reading-time": "1.5.0",
        "rehype-autolink-headings": "7.1.0",
        "shiki": "1.12.1",
        "rehype-pretty-code": "0.13.1",
        "rehype-slug": "6.0.0",
        "tailwind-merge": "2.2.2",
        "ts-pattern": "5.2.0",
        "@vercel/speed-insights": "1.0.10"
    },
    "devDependencies": {
        "@astrojs/check": "0.7.0",
        "@typescript-eslint/eslint-plugin": "7.18.0",
        "@typescript-eslint/parser": "7.18.0",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-astro": "0.34.0",
        "prettier": "3.3.3",
        "prettier-plugin-astro": "0.14.0",
        "tailwindcss": "3.4.1",
        "typescript": "5.5.4",
        "vercel": "35.2.2"
    }
}
