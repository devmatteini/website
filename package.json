{
    "name": "website",
    "type": "module",
    "version": "2.0.0",
    "scripts": {
        "dev": "astro dev",
        "start": "astro preview",
        "build": "astro build",
        "typecheck": "tsc --noEmit",
        "typecheck:w": "npm run typecheck -- --watch",
        "check": "astro check",
        "check:w": "npm run check -- --watch",
        "format": "prettier -w src config",
        "format:check": "prettier --check src/**/*.{js,ts,tsx,astro} config/**/*.ts",
        "lint": "eslint --ext .js,.ts,.tsx,.astro src config",
        "ci:setup-vercel": "vercel pull --yes --environment production --token \"$VERCEL_TOKEN\"",
        "ci:deploy": "vercel deploy --prebuilt --prod --token \"$VERCEL_TOKEN\""
    },
    "dependencies": {
        "@astrojs/mdx": "2.0.3",
        "@astrojs/tailwind": "5.1.0",
        "@astrojs/vercel": "6.1.3",
        "@fontsource-variable/inter": "5.0.16",
        "astro": "4.3.0",
        "astro-icon": "1.0.2",
        "clsx": "2.1.0",
        "reading-time": "1.5.0",
        "rehype-autolink-headings": "7.1.0",
        "shikiji": "0.9.17",
        "rehype-pretty-code": "0.12.6",
        "rehype-slug": "6.0.0",
        "tailwind-merge": "2.2.1",
        "ts-pattern": "5.0.6",
        "unist-util-visit": "5.0.0",
        "@vercel/speed-insights": "1.0.9"
    },
    "devDependencies": {
        "@astrojs/check": "0.4.1",
        "@typescript-eslint/eslint-plugin": "6.20.0",
        "@typescript-eslint/parser": "6.20.0",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-astro": "0.31.4",
        "prettier": "3.1.1",
        "prettier-plugin-astro": "0.12.3",
        "tailwindcss": "3.4.1",
        "typescript": "5.3.3",
        "vercel": "33.4.1"
    }
}
